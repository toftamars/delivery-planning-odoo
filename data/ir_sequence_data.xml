<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Teslimat Planlama Sequence -->
        <record id="seq_delivery_planning" model="ir.sequence">
            <field name="name">Teslimat Planlama</field>
            <field name="code">delivery.planning</field>
            <field name="prefix">DEL/%(y)s/</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
        </record>

        <!-- Teslimat Belgesi Sequence -->
        <record id="seq_delivery_document" model="ir.sequence">
            <field name="name">Teslimat Belgesi</field>
            <field name="code">delivery.document</field>
            <field name="prefix">DOC/%(y)s/</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>
